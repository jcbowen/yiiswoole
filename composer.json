{
  "name": "jcbowen/yiiswoole",
  "description": "Yii websocket server component",
  "keywords": [
    "yii",
    "yii2",
    "swoole",
    "websocket",
    "php",
    "jcbowen"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "jcbowen",
      "email": "bowen@jiuchet.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": "^2.0",
    "ext-posix": "*",
    "ext-json": "*",
    "jcbowen/jcbase-yii2": "^0.x-dev || ^0.3.4"
  },
  "autoload": {
    "psr-4": {
      "Jcbowen\\yiiswoole\\": "./src"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
